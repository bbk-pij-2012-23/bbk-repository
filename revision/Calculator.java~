public class Calculator {
	
	public int add(int a, int b) {
		int sum = a + b;
		return sum;
	}
	public int subtract(int a, int b) {
		int equals = a - b;
		return equals;
	}
	public int multiply(int a, int b) {
		int equals = a * b;
		return equals;
	}
	public int divide(int a, int b) {
		int equals = a / b; 
		return equals;
	}
	public int modulus(int a, int b) {
		int equals = a % b;
		//print " remainder "
		return equals;
	}	
		
}
 